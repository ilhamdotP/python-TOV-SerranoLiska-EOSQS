size 12 18                             !Sets the size of the paper

set texlabels 1

amove 0.5 9.5
begin graph
        size 12 9                      !Sets the size of the graph
		data "nonTOV/0-B=57,BT=57,a4=0.7,a4T=0.7,w=0.333,ct=1e0/profileSL_mod.csv"   d30=c1,c5 
		data "nonTOV/6-B=57,BT=57,a4=0.7,a4T=0.7,w=1,ct=1e0/profileSL_mod.csv"   d36=c1,c5 
		data "nonTOV/9-B=57,BT=57,a4=0.7,a4T=0.7,w=0.333,ct=1e7/profileSL_mod.csv"   d40=c1,c5 
		data "nonTOV/14-B=57,BT=57,a4=0.7,a4T=0.7,w=1,ct=1e7/profileSL_mod.csv"   d46=c1,c5 
		yaxis min 0.000000000001 max 10. hei .5 nticks 5 log grid
		xaxis min 0 max 15 dticks 3 hei .5 grid
		ytitle "$|\Lambda| (10^{30} $ g$^2$ cm$^{-6})$"  hei .5
        xtitle "$r ($km$)$" hei .5
		d30 line lstyle 1 lwidth 0.06 color red
		d36 line lstyle 3 lwidth 0.06 color orange
		d40 line lstyle 2 lwidth 0.10 color blue
		d46 line lstyle 4 lwidth 0.10 color brown
        !key pos tl hei 0.4
		ticks color gray10
end graph 

begin key
position tl
hei .35
 nobox
end key

begin key
position bl
hei .35
 nobox
  !text "$P_c=500$ MeV/fm$^3$"
