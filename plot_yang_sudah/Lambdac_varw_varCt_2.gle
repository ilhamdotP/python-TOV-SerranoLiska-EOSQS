  text "$B=B_{\perp}=1.02\times10^{14}$ g/cm$^3$"
  text "$a_{4}=a_{4\perp}=0.7$"
		line lstyle 1 lwidth 0.06 color red          text "$w=1/3,\tilde{c}=1$ m$^2$"
		line lstyle 3 lwidth 0.06 color orange          text "$w=1,\tilde{c}=1$ m$^2$"
		line lstyle 2 lwidth 0.10 color blue            text "$w=1/3,\tilde{c}=10^7$ m$^2$"
		line lstyle 4 lwidth 0.10 color brown           text "$w=1,\tilde{c}=10^7$ m$^2$"
end key

begin key
position br
hei .35
 nobox
  text "(a)"
end key

